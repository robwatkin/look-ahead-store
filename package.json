{
  "name": "look-ahead-store",
  "version": "0.0.8",
  "description": "Simple look ahead key value store",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/robwatkin/look-ahead-store.git",
  "author": "Rob Watkin <robwatkin@gmail.com>",
  "license": "Apache License 2.0",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "rm -rf dist && tsc",
    "dev": "nodemon",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "ts-node": "^9.1.1",
    "typescript-is": "^0.18.2"
  }
}
